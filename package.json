    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "@xyflow/react": "^12.3.2",
    "antd": "^5.21.5",
    "axios": "^1.7.7",
    "d3-array": "^3.2.4",
    "d3-drag": "^3.0.0",
    "d3-selection": "^3.0.0",
    "i": "^0.3.7",
    "i18next": "^23.16.4",
    "i18next-browser-languagedetector": "^8.0.0",
    "immer": "^10.1.1",
    "nanoid": "^5.0.7",
    "npm": "^10.9.0",
    "react": "^18.3.1",
    "react-csv": "^2.2.2",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.1.0",
    "react-router-dom": "^6.27.0",
    "react18-json-view": "^0.2.8",
    "recoil": "^0.7.7",
    "styled-components": "^6.1.13",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.10.0",
    "vite": "^5.4.9"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.{ts,tsx,css,json,scss,css}": [
      "prettier --write"
